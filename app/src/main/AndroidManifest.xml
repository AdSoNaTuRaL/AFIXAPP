        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".tareas.updateDelete"></activity>
        <activity android:name=".menu_principal.Profile" />
        <activity android:name=".menu_principal.Informacion" />
        <activity android:name=".menu_principal.LoginActivity" />
