        <activity android:name=".menu_principal.Configuracion" />
        <activity android:name=".menu_principal.Juegos" />
        <activity android:name=".menu_principal.Maps" />
        <activity android:name=".menu_principal.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".relajacion.GalleryActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />
    </application>

</manifest>